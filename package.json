{
  "name": "personal-task-manager",
  "version": "1.0.0",
  "description": "A beautiful and functional personal task manager web application",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "test": "NODE_ENV=test node --test test/*.test.js",
    "test:watch": "NODE_ENV=test node --test --watch test/*.test.js",
    "test:verbose": "NODE_ENV=test node --test --test-reporter=spec test/*.test.js",
    "test:storage": "NODE_ENV=test node --test test/storage.test.js",
    "test:api": "NODE_ENV=test node --test test/tasks.test.js"
  },
  "keywords": [
    "task-manager",
    "todo",
    "productivity",
    "web-app"
  ],
  "author": "AI Assistant",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "supertest": "^6.3.3"
  }
}
